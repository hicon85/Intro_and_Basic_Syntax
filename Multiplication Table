using System;

namespace _10.Multiplication_Table
{
    class Program
    {
        static void Main(string[] args)
        {
            double integer = double.Parse(Console.ReadLine());

            double times = 1;

            while (times <= 10)
            {
                Console.WriteLine($"{integer} X {times} = {integer * times}");
                times++;
            }
        }
    }
}
